{
  "name": "afari-real-estate-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "build": "npx prisma generate && npx prisma db push",
    "prisma:gen": "prisma generate --schema ./prisma/schema.prisma",
    "prisma:push": "prisma db push --schema ./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --name init",
    "render-postbuild": "npx prisma generate && npx prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.7",
    "nodemailer": "^6.9.13"
  },
  "devDependencies": {
    "eslint-plugin-node": "^11.1.0",
    "nodemon": "^3.1.0",
    "prisma": "^6.15.0"
  }
}
